			/********************************************************************************************************************/
			/* 01. 공통라이브러리 정의																					*/
			/********************************************************************************************************************/
			
			
			/********************************************************************************************************************/
			/* 02. Form 변수정의																								*/
			/********************************************************************************************************************/
        	var selectBoxValue;
        	
        	/********************************************************************************************************************/
			/* 03. Form 화면 초기화																							*/
			/********************************************************************************************************************/
			function fn_onload(){
				gfn_callCommonCode(
				"COM",
				["0003"],
				["dl_userDivisionCode"]
			);
			}
			
			/********************************************************************************************************************/
			/* 04. 서비스 TRANSACTION 함수(조회,저장,삭제,Callback)																		*/
			/********************************************************************************************************************/
			//조회
			function fn_search(){
				$w.executeSubmission( "searchSubmission" );
				//selectBoxValue = selectbox2.getValue()
			}
			
			//저장
			function fn_save(){
				var modifiedData = dl_selectUser.getModifiedJSON();
				alert("modifiedData =============== " + JSON.stringify(modifiedData));
				
				$w.executeSubmission("modifiedSubmission");
			}
			
			/**************************************************************************
			 * 05.공통버튼 영역
			***************************************************************************/
        	
        	/**************************************************************************
			 * 06.그리드 행추가, 삭제, 엑셀출력 버튼영역
			***************************************************************************/
        	//행추가
        	function fn_addRow(){
        		var rowIndex = grd_GridView.getFocusedRowIndex();
                dl_selectUser.insertRow( rowIndex + 1 );
                grd_GridView.setFocusedCell( rowIndex + 1 , 1 , false )
            }
        	
        	//행삭제
        	function fn_delRow(){
        		try{
	    			var dataListStr = grd_GridView.getDataList(); // return string type
	    			var dataList = WebSquare.util.getComponentById( dataListStr );
	    			
	    			var chkIndex = dl_selectProgram.getMatchedIndex( "chk" ,1 ,true ,0 ,dl_selectProgram.getRowCount() );
	    			if(chkIndex.length > 0){
		 				for(var i=0; i<chkIndex.length; i++){
		 					dataList.deleteRow( chkIndex[i] );
		 				}
	 				}else{
	 					alert("삭제할 데이터를 체크하십시오.");
	 				}
	 			}catch(e){
	 			}finally{
	 				dataList = null; //메모리 초기화
	 			}
        	}
        	
        	//바로행삭제
        	function fn_delNowRow(){
        		try{
	    			var dataListStr = gridView1.getDataList(); // return string type
	    			var dataList = WebSquare.util.getComponentById( dataListStr );
	    			
	    			var chkIndex = dl_selectUser.getMatchedIndex( "chk" ,1 ,true ,0 ,dl_selectUser.getRowCount() );
	    			
	    			if(chkIndex.length > 0){
						dataList.removeRows( chkIndex );
	 				}else{
	 					alert("삭제할 데이터를 체크하십시오.");
	 				}
	 			}catch(e){
	 			}finally{
	 				dataList = null; //메모리 초기화
	 			}
        	}
        	
        	// 엑셀다운로드 (API문서 참고)
			function fn_exportExcel(grdViewObj, options, infoArr) {
			    grdViewObj.advancedExcelDownload(options, infoArr);
			};
			
