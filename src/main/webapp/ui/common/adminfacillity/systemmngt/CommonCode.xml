			//조회조건 체크
			function fn_searchCheck()
			{
				if(gfn_isEmpty(slb_SysCode.getValue())){
					gfn_Message("COM", "1064", "AF", ["시스템구분"]);
					return false;
				}
				/*
				if(gfn_isEmpty(ipb_ComCodeNm.getValue())){
