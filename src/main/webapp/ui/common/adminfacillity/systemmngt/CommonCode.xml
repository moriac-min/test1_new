					return false;
				}
				*/
				return true;
			}
			
			//조회
			function fn_search()
			{
				if(fn_searchCheck()){
//					grd_CommonCode.initGrid();
//					grd_CommonCodeData.initGrid();
					gfn_datalistClear(dl_CommonCode);
					gfn_datalistClear(dl_CommonCodeData);
//					var tmpJson = "[]";
//					dl_CommonCode.setJSON( JSON.parse(tempJson) , false );
//					dl_CommonCodeData.setJSON( JSON.parse(tempJson) , false );
//					dl_CommonCode.removeAll();
//					dl_CommonCodeData.removeAll();
					$w.executeSubmission( "sub_SelectCommonCode" );
				}
			}
			
			//상세조회
			function fn_searchData(sRow, oRow)
			{
				if(dl_CommonCodeData.getModifiedIndex() > 0)
				{
					//저장하지 않은 데이타가 있습니다. 계속하시겠습니까?
					if(gfn_Message("COM", "1022", "C"))
					{
						grd_CommonCode.setFocusedCell( oRow , 0 , false );
						return;
					}
				}
				
				if(sRow > -1 && dl_CommonCode.getRowStatus( sRow ) != "C")
				{
					var selectSysCd = dl_CommonCode.getCellData( sRow , "sys_cd" );
					var selectCmmnCd = dl_CommonCode.getCellData( sRow , "cmmn_cd" );
					if(!gfn_isEmpty(selectSysCd) && !gfn_isEmpty(selectCmmnCd))
					{
						dm_CommonCodeData.set("sys_cd", selectSysCd);
						dm_CommonCodeData.set("cmmn_cd", selectCmmnCd);
						$w.executeSubmission( "sub_SelectCommonCodeData" );
					}
				} else if(sRow > -1 && dl_CommonCode.getRowStatus( sRow ) == "C")
				{
//					var tmpJson = "[]";
//					dl_CommonCodeData.setJSON( JSON.parse(tempJson) , false );
					gfn_datalistClear(dl_CommonCodeData);
//					grd_CommonCodeData.initGrid();
//					dl_CommonCodeData.removeAll();
				}
			}
			
