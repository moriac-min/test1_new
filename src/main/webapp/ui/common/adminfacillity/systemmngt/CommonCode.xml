        	//행추가
        	function fn_addRow()
        	{
        		if(gfn_isEmpty(fv_searchSysCd)){
        			gfn_Message("COM", "1102", "AF", ["추가할 시스템 코드값"]);
        			return;
        		}
                if(dl_CommonCodeData.getModifiedIndex().length > 0)
				{
					//저장하지 않은 데이타가 있습니다. 계속하시겠습니까?
					if(gfn_Message("COM", "1022", "C"))
					{
						return;
					} else 
					{
						var rowIndex = grd_CommonCode.getFocusedRowIndex();
		                dl_CommonCode.insertRow( rowIndex + 1 );
		                dl_CommonCode.setCellData( rowIndex + 1 , 'sys_cd' , fv_searchSysCd );
		                
		                grd_CommonCode.setReadOnly( 'cell' , rowIndex , 'cmmn_cd' , false );
					}
				} else
				{
					var rowIndex = grd_CommonCode.getFocusedRowIndex();
	                dl_CommonCode.insertRow( rowIndex + 1 );
	                dl_CommonCode.setCellData( rowIndex + 1 , 'sys_cd' , fv_searchSysCd );
	                
	                grd_CommonCode.setReadOnly( 'cell' , rowIndex , 'cmmn_cd' , false );
				}
				
        	}
        	
        	//행삭제
        	function fn_delRow()
        	{
        		try 
        		{	    			
	 				for(var i=0; i<dl_CommonCodeData.getRowCount(); i++)
	 				{
	 					dl_CommonCodeData.deleteRow( i );
	 				}
	 				dl_CommonCode.deleteRow( grd_CommonCode.getFocusedRowIndex() );
	 			} catch(e)
	 			{
	 			}
        	}
        	
        	//행바로삭제
        	function fn_delNowRow()
        	{
        		return;
        	}
        	
        	//상세행추가
        	function fn_addDataRow()
        	{
        		if(grd_CommonCode.getFocusedRowIndex() > -1){
        			//시스템코드와 공통코드가 정의되어있어야 상세행추가가 된다.
        			if(!gfn_isEmpty(dl_CommonCode.getCellData( grd_CommonCode.getFocusedRowIndex() , "sys_cd" )) && !gfn_isEmpty(dl_CommonCode.getCellData( grd_CommonCode.getFocusedRowIndex() , "cmmn_cd" )))
        			{
        				var rowIndex = grd_CommonCodeData.getFocusedRowIndex();
		                dl_CommonCodeData.insertRow( rowIndex + 1 );
		                dl_CommonCodeData.setCellData( rowIndex+1 , "sys_cd" , dl_CommonCode.getCellData( grd_CommonCode.getFocusedRowIndex() , "sys_cd" ) );
		                dl_CommonCodeData.setCellData( rowIndex+1 , "cmmn_cd" , dl_CommonCode.getCellData( grd_CommonCode.getFocusedRowIndex() , "cmmn_cd" ) );
		                dl_CommonCodeData.setCellData( rowIndex+1 , "use_yn" , "Y");
        			} else 
        			{
        				gfn_Message("COM", "1020", "AF", ["상위 데이터의 시스템코드, 공통코드"]);
        				return;
        			}
                } else {
                	gfn_Message("COM", "1064", "AF", ["공통코드 항목"]);
                	return;
                }
        	}
        	
        	//상세행삭제
        	function fn_delDataRow()
        	{
        		try 
        		{
	    			var dataListStr = grd_CommonCodeData.getDataList(); // return string type
	    			var dataList = WebSquare.util.getComponentById( dataListStr );
	    			
	    			var chkIndex = dl_CommonCode.getMatchedIndex( "chk" ,1 ,true ,0 ,dl_CommonCodeData.getRowCount() );
	    			if(chkIndex.length > 0)
	    			{
		 				for(var i=0; i<chkIndex.length; i++)
		 				{
		 					dataList.deleteRow( chkIndex[i] );
		 				}
	 				}else
	 				{
	 					gfn_Message("COM", "1145", "A", ["삭제할 데이터"]);
	 				}
	 			} catch(e)
	 			{
	 			} finally
	 			{
	 				dataList = null; //메모리 초기화
	 			}
        	}
        	
