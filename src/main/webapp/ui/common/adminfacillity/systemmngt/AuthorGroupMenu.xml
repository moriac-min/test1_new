			function fn_checkAllColumn(sRow,sCol,sVal)
			{
				alert("fn_checkAllColumn::sRow="+sRow+",sCol="+sCol+",sVal="+sVal);
				if(sRow > -1)
				{
					dl_GrpMenu.setCellData( sRow , 'inqry_athr_yn' , sVal );
