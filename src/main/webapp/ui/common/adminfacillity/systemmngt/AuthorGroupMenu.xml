        	var fv_searchAthrGrpCd = "";
        	var fv_searchAthrGrpNm = "";
        	var fv_searchWorkDvsCd = "";
        	var fv_selectAthrGrpCd = "";
        	var fv_selectAthrGrpNm = "";
        	var fv_searchSysCd = "";
        	
        	/********************************************************************************************************************/
			/* 03. Form 화면 초기화																							*/
			/********************************************************************************************************************/
			
			gfn_callCommonCode(
				"COM",
				["0001"],
				["dl_COM0001"]
			);
			
        	function fn_reset()
        	{
        		gfn_initGroup(grp_Search);
        	}
        	/********************************************************************************************************************/
			/* 04. 서비스 TRANSACTION 함수(조회,저장,삭제,Callback)																		*/
			/********************************************************************************************************************/
			//조회조건 체크
			function fn_searchCheck()
			{
				if(gfn_isEmpty(slb_SysCd.getValue())){
					alert("시스템구분을 선택해주시기 바랍니다.");
					return false;
				}
				return true;
			}
			
			//조회
			function fn_search()
			{
				if(fn_searchCheck()){
					gfn_datalistClear(dl_AttrGrp);
					gfn_datalistClear(dl_GrpMenu);
					
					$w.executeSubmission( "sub_SearchAttrGrp" );
				}
			}
			
			//그룹별 메뉴권한 조회
			function fn_searchData(sRow, oRow)
			{
				//메뉴를 업무구분을 선택하고 조회했을 경우
				if(gfn_isEmpty(sRow) || gfn_isEmpty(oRow))
				{
					sRow = grd_AthrGrp.getFocusedRowIndex();
					oRow = grd_AthrGrp.getFocusedRowIndex();
				}
				
				if(!confirm("저장하지 않은 데이터가 있을경우 사라집니다. 계속 진행하시겠습니까?"))
				{
					grd_AthrGrp.setFocusedCell( oRow , 0 , false );
					return;
				}
				
				fv_selectAthrGrpCd = dl_AttrGrp.getCellData( sRow , "athr_grp_cd" );
				fv_selectAthrGrpNm = dl_AttrGrp.getCellData( sRow , "athr_grp_nm" );
//				alert("선택값[" + fv_selectAthrGrpCd + ":::" + fv_selectAthrGrpNm + "]");
				if(!gfn_isEmpty(fv_selectAthrGrpCd))
				{
					dm_GrpMenu.set("athr_grp_cd", fv_selectAthrGrpCd);
					dm_GrpMenu.set("sys_cd", fv_searchSysCd);
					var vLabelMsg = fv_selectAthrGrpNm + " 그룹의";
					txb_AhtrGrpNm.setLabel( vLabelMsg );
					$w.executeSubmission( "sub_SearchGrpMenu" );
				}
			}
			
			// 저장
			function fn_save()
			{
				if(dl_GrpMenu.getModifiedData().length > 0){
					if(confirm("저장하시겠습니까?"))
					{
						dm_GrpMenu.set("athr_grp_cd", fv_selectAthrGrpCd);
						dm_GrpMenu.set("sys_cd", fv_selectAthrGrpCd);
						dm_GrpMenu.set("sys_cd", fv_searchWorkDvsCd);
						
						$w.executeSubmission("sub_SaveGrpMenu");
						
					} else 
					{
						return;
					}
				} else
				{
					alert("수정된 데이터가 없습니다.");
				}	
			}
			
			//저장후계속
			function fn_saving()
			{
				return;
			}
			
			//CallBack
			function fn_callBack(sId, e)
			{
				//TODO 
				//추후 메시지 함수가 들어갈 예정
				if(sId == "sub_SearchAttrGrp")
				{
					fv_searchSysCd = slb_SysCd.getValue();
		        	fv_searchAthrGrpCd = ipb_AthrGrpCd.getValue();
		        	fv_searchAthrGrpNm = ipb_AthrGrpNm.getValue();
		        	
					if(dl_AttrGrp.getRowCount() < 1){
						txb_Count.setValue( "0" );
						gfn_datalistClear(dl_AttrGrp);
					} else {
						txb_Count.setValue( dl_AttrGrp.getRowCount() );
					}
				} else if(sId == "sub_SearchGrpMenu")
				{
					
				} else if(sId == "sub_SaveGrpMenu")
				{
					
				}
			}
			
			//ErrorCallBack
			function fn_errorCallBack(sId, e)
			{
				//TODO
				//추후 메시지 함수가 들어갈 예정
				if(sId == "sub_SearchOrganization")
				{
					alert("[sub_SearchOrganization] 실행에 에러가 발생하였습니다.");
				} else if(sId == "sub_SearchOrganizationAuthorGroup")
				{
					alert("[sub_SearchOrganizationAuthorGroup] 실행에 에러가 발생하였습니다.");
				} else if(sId == "sub_SaveOrganizationAuthorGroup")
				{
					
					alert("[sub_SaveOrganizationAuthorGroup] 실행에 에러가 발생하였습니다.");
				}
			} 
			
