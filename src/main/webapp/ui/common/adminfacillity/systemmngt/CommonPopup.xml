        <script type="javascript"><![CDATA[ 
        /***************************************************************************************
			* 시스템명 		: 식품의약품안전처 통합식품안전망구축
			* 업무명 		: CommonPopup
			* 파일명 		: CommonPopup.xml
			* 작성자 		: 유승엽
			* 작성일 		: 2014.09.05
			*
			* 설  명		: 공통팝업 관리 
			*---------------------------------------------------------------------------------------
			* 변경일	변경자		변경내역 
			*---------------------------------------------------------------------------------------
			*  2014.09.05 유승엽	최초 프로그램 작성         
			* 
			*---------------------------------------------------------------------------------------
			****************************************************************************************/
			
			/********************************************************************************************************************/
			/* 01. 공통라이브러리 정의																					*/
			/********************************************************************************************************************/
			
			
			/********************************************************************************************************************/
			/* 02. Form 변수정의																								*/
			/********************************************************************************************************************/
        	var selectBoxValue = '';
        	
        	/********************************************************************************************************************/
			/* 03. Form 화면 초기화																							*/
			/********************************************************************************************************************/
			gfn_callCommonCode(
				"COM",
				["0001"],
				["dl_systemCode"]
			);
			/********************************************************************************************************************/
			/* 04. 서비스 TRANSACTION 함수(조회,저장,삭제,Callback)																		*/
			/********************************************************************************************************************/
			//조회
			function fn_search(){
				if(gfn_isEmpty(slb_Syscd.getValue())){
					alert("시스템구분을 선택해주시기 바랍니다.");
					return false;
				}
				$w.executeSubmission("searchSubmission");
				selectBoxValue = slb_Syscd.getValue();
			}
			
			//상세조회
			function fn_searchDetail(row){
				dm_subSearchKeyword.set('sys_cd', dl_Popup.getCellData( row , 'sys_cd' ));
				dm_subSearchKeyword.set('popup_cd', dl_Popup.getCellData( row , 'popup_cd' ));
				
				if(dl_Popup.getRowStatus( row ) != 'C'){	
					$w.executeSubmission("searchDetailSubmission");
				}
				else{
					gfn_datalistClear(dl_PopupItem);
				}
			}
			
			//저장
			function fn_save(){
			var modifiedData = dl_Popup.getModifiedIndex();
			var nullcheck = 0;
                for(var i=0; i<modifiedData.length; i++){
		 			var mssg_cd = dl_Popup.getCellData( modifiedData[i] , 'popup_cd' );
		 		if(mssg_cd == '' || mssg_cd == null){
		 			nullcheck = 1;
		 		}
		 	}
		 	if(nullcheck == 0)
				$w.executeSubmission("modifiedSubmission");	
			if(nullcheck == 1)
				alert("필수입력항목 오류");
			}
			
			/**************************************************************************
			 * 05.공통버튼 영역
			***************************************************************************/
        	
        	/**************************************************************************
			 * 06.그리드 행추가, 삭제, 엑셀출력 버튼영역
			***************************************************************************/
        	//행추가
        	function fn_addRow(){
	        	if(gfn_isEmpty(selectBoxValue)){
        			alert("추가할 시스템 코드값으로 조회하여주시기 바랍니다.");
        			return;
        		}
        		var rowIndex = grd_Popup.getFocusedRowIndex();
                dl_Popup.insertRow( rowIndex + 1 );
                dl_Popup.setCellData( rowIndex + 1 , 'sys_cd' , selectBoxValue );
                grd_Popup.setCellDisabled(rowIndex + 1 , 'popup_cd' , false);
                grd_Popup.setFocusedCell( rowIndex + 1 , 'popup_cd' , true );
                
        	}
        	
        	//행삭제
        	function fn_delRow(){
        		try{
	    			if(grd_Popup.getFocusedRowIndex() != -1){
		 				for(var i=0; i<dl_PopupItem.getRowCount(); i++){
		 					dl_PopupItem.deleteRow( i );
		 				}
		 				dl_Popup.deleteRow(grd_Popup.getFocusedRowIndex());
	 				}else{
	 					gfn_Message("COM", "1081", "A");
	 					alert("삭제할 데이터를 체크하십시오.");
	 				}
	 			}catch(e){
	 			}
        	}
        	
        	//상세행추가
        	function fn_addRowDetail(){
	        	var max_itm_ordno = 0;
	        	var popupRowIndex = grd_Popup.getFocusedRowIndex();
	        	var rowIndex = grd_PopupItem.getFocusedRowIndex();
	        	
	        	if(dl_Popup.getCellData( popupRowIndex , 'popup_cd' ) == ''){
	        		alert("팝업코드를 입력해 주시기 바랍니다.");
	        		return false;
	        	}
	        	grd_Popup.setCellDisabled( popupRowIndex , 'popup_cd' , true);
	        	
	        	if(gfn_isEmpty(selectBoxValue)){
        			alert("추가할 시스템 코드값으로 조회하여주시기 바랍니다.");
        			return;
        		}
        		
                dl_PopupItem.insertRow( rowIndex + 1 );
                dl_PopupItem.setCellData( rowIndex + 1 , 'sys_cd' , dl_Popup.getCellData( popupRowIndex , 'sys_cd' ) );
                dl_PopupItem.setCellData( rowIndex + 1 , 'popup_cd' , dl_Popup.getCellData( popupRowIndex , 'popup_cd' ) );
        	}
        	
        	//상세행삭제
        	function fn_delRowDetail(){
        		try{
	    			if(grd_PopupItem.getFocusedRowIndex() != -1){
		 				dl_PopupItem.deleteRow(grd_PopupItem.getFocusedRowIndex());
	 				}else{
	 					gfn_Message("COM", "1081", "A");
	 					alert("삭제할 데이터를 체크하십시오.");
	 				}
	 			}catch(e){
	 			}
        	}
        	
        	// 엑셀다운로드 (API문서 참고)
			function fn_exportExcel(grdViewObj, options, infoArr) {
			    grdViewObj.advancedExcelDownload(options, infoArr);
			};
			
        	/**************************************************************************
			 * 07. 사용자 정의 함수 구현 영역
			***************************************************************************/
        ]]></script>
